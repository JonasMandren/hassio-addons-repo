{
  "name": "Sonarr",
  "version": "latest",
  "slug": "sonarr_docker",
  "description": "Sonarr add-on based on the prebuilt docker image from linuxserver",
  "url": "https://github.com/marcelveldt/hassio-addons-repo/sonarr",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "8989/tcp": 8989
  },
  "map": [
    "backup:rw",
    "share:rw",
    "mnt:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8989]",
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {}
}
